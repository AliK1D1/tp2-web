
<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"] - Combattant UFC</title>

    
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">

    
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">

   
    <link rel="stylesheet" type="text/css" href="~/CSS/style.css" asp-append-version="true">
</head>
<body class="rechercheback">

    
    <header>
        <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
            <div class="container">
                <a href="/home/Index" class="navbar-brand" asp-controller="Home" asp-action="Index"><i class="fas fa-fist-raised"></i> UFC</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav ms-auto">
                        <li class="nav-item">
                            <a href="/home/Index" class="nav-link" asp-controller="Home" asp-action="Index"><i class="fas fa-home"></i> ACCUEIL</a>
                        </li>
                        <li class="nav-item">
                            <a href="/Favoris/Index" class="nav-link" asp-controller="Favoris" asp-action="Index"><i class="fas fa-star"></i> FAVORIS</a>
                        </li>
                        <li class="nav-item">
                            <a href="/Enfant/recherche" class="nav-link" asp-controller="Recherche" asp-action="Index"><i class="fas fa-search"></i> RECHERCHE</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>

   
    <main>
        <div class="container mt-4">
            @RenderBody()
        </div>
    </main>

    
    <footer class="bg-dark text-white text-center py-3 mt-4">
        <p>&copy; 2025 UFC. Tous droits réservés.</p>
    </footer>

    
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous"></script>
</body>
</html>

